>EURORADIO.LastKnownRBC &lt;- EURORADIO.RBCContactStruct
{
    Id =&gt; anOrder.Id,
    PhoneNumber =&gt; anOrder.PhoneNumber,
    DataState =&gt; DataStateEnum.Valid
}</Action><Action
 Guid="b327d97a-1053-46ca-a7eb-480617cdb643"
>EURORADIO.RBCContactOrderToRepeat &lt;- EMPTY</Action><Action
